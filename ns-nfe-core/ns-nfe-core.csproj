<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>ns_nfe_core</RootNamespace>
    <StartupObject>ns_nfe_core.Program</StartupObject>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Integração @ NS Tecnologia</Authors>
    <Company>NS Tecnologia</Company>
    <Description>Biblioteca para comunicação com a API de NFe da NS Tecnologia.</Description>
    <Copyright>NS Tecnologia</Copyright>
    <RepositoryUrl>https://github.com/NSTecnologia/ns-nfe-core</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <Version>1.0.9</Version>
    <PackageIcon>logo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
